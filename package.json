    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.20.0",
    "postcss": "^8.5.3",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vue-tsc": "^2.2.0"
